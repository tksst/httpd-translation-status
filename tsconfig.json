{
    "compilerOptions": {
        "target": "ES5",
        "lib": [
            "dom",
            "es5",
            "es2015.promise",
            "es2017.object"
        ],
        "jsx": "react",
        "alwaysStrict": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "sourceMap": true,
        "module": "es2015"
    }
}
